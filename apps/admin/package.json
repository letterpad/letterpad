{
  "name": "admin",
  "version": "4.1.2",
  "author": "Abhishek Saha",
  "license": "MIT",
  "prisma": {
    "seed": "node ./src/graphql/db/dist/index.js"
  },
  "eslint": {
    "dirs": [
      "src"
    ]
  },
  "scripts": {
    "seedIfNotSeeded": "bun run buildSeed && node ./src/graphql/db/dist/seedIfNoSeed.js",
    "prepare": "bun run prisma:generate && bun run prisma:migrate:run",
    "clean": "rm -rf .next",
    "client": "concurrently -k 'bun dev'",
    "dev": "next",
    "buildSeed": "tsup ./src/graphql/db/seed/seedIfNoSeed.ts ./src/graphql/db/seed/index.ts --format cjs --outDir ./src/graphql/db/dist",
    "build": "next build",
    "test": "jest --runInBand",
    "start": "next start",
    "seed": "bun run buildSeed && bun run prisma:seed",
    "lint": "next lint",
    "lint:fix": "next lint --fix && bun run format",
    "migrate": "prisma migrate dev --name init",
    "typecheck": "bun run tsc",
    "prisma:generate": "dotenv scripts/generatePrisma.sh",
    "prisma:migrate:create": "dotenv scripts/createMigration.sh",
    "prisma:migrate:run": "dotenv scripts/runMigration.sh",
    "prisma:seed": "node ./src/graphql/db/dist/index.js",
    "prisma:studio": "dotenv scripts/studio.sh",
    "cy:open": "cypress open --env SECRET_KEY='some-secret' --browser chrome",
    "cy:run": "cypress run --record --key e2c7bae0-47ed-49e5-8ea8-b0c26c3493f7 --browser chrome",
    "e2e:ci": "bun run buildSeed && dotenv scripts/seed.sh && start-server-and-test 'bun start' http://localhost:3000/login cy:run",
    "e2e": "dotenv scripts/seed.sh && start-server-and-test 'bun dev' http://localhost:3000/login cy:open",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "find-deadcode": "ts-prune | grep -Ev \"\\.d\\.ts|__generated__|seed\" | cat"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.0.5",
    "@dnd-kit/sortable": "^7.0.1",
    "@dnd-kit/utilities": "^3.2.0",
    "ramda": "0.29.1",
    "@floating-ui/react-dom-interactions": "^0.10.2",
    "@graphql-tools/utils": "^8.10.0",
    "@prisma/client": "^5.4.1",
    "@stripe/stripe-js": "^1.42.1",
    "@tinymce/tinymce-react": "^4.3.0",
    "@types/bcryptjs": "^2.4.2",
    "@graphql-yoga/plugin-response-cache": "3.2.0",
    "@urql/exchange-graphcache": "^6.4.0",
    "@urql/introspection": "^1.0.2",
    "@urql/next": "^1.1.0",
    "axios": "^1.2.0",
    "bcryptjs": "^2.4.3",
    "jose": "5.2.0",
    "cheerio": "^1.0.0-rc.10",
    "classnames": "^2.3.2",
    "clipboard": "^2.0.11",
    "cloudinary": "^1.40.0",
    "copy-dir": "^1.3.0",
    "cross-fetch": "^3.1.5",
    "cryptr": "^6.0.2",
    "dataloader": "^2.2.2",
    "debug": "^4.3.1",
    "dotenv": "^16.0.1",
    "dotenv-cli": "^5.1.0",
    "eslint-config-custom": "workspace:*",
    "graphql": "16.6.0",
    "graphql-request": "^3.4.0",
    "graphql-yoga": "^5.1.1",
    "image-size": "^0.9.7",
    "md5": "^2.3.0",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
    "next-auth": "^4.24.5",
    "next-nprogress-bar": "^2.1.0",
    "nodemailer": "^6.9.8",
    "prisma": "5.10.2",
    "react-colorful": "^5.6.1",
    "react-google-recaptcha-v3": "^1.10.0",
    "react-hook-form": "^7.45.2",
    "react-infinite-scroll-component": "^6.1.0",
    "react-sticky-box": "^1.0.2",
    "react-tag-autocomplete": "^6.3.0",
    "showdown": "^2.0.0",
    "sqlite3": "^5.0.11",
    "ui": "workspace:*",
    "streamifier": "^0.1.1",
    "stripe": "^10.15.0",
    "swr": "1.3.0",
    "tinymce": "6.7.2",
    "urql": "^4.0.6",
    "openai": "4.24.1",
    "ai": "2.2.30",
    "@aws-sdk/client-ses": "3.504.0",
    "canvas-confetti": "^1.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.1",
    "@commitlint/config-conventional": "^17.0.0",
    "@graphql-tools/jest-transform": "^2.0.0",
    "@types/gtag.js": "^0.0.12",
    "@types/jest": "^29.5.3",
    "@types/multer": "^1.4.5",
    "@types/nodemailer": "^6.4.14",
    "@types/nprogress": "^0.2.0",
    "@types/react-test-renderer": "^18.0.0",
    "@types/react-tooltip": "^4.2.4",
    "@types/ramda": "0.29.9",
    "concurrently": "^6.2.0",
    "config": "workspace:*",
    "cypress": "13.2.0",
    "letterpad-graphql": "workspace:*",
    "cypress-plugin-tab": "^1.0.5",
    "graphql-tag": "2.11.0",
    "html-webpack-plugin": "5.5.0",
    "jest": "^29.6.1",
    "lint-staged": "^12.4.2",
    "node-env-file": "^0.1.8",
    "react-test-renderer": "18.2.0",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^29.1.1",
    "tsconfig": "workspace:*",
    "tsconfig-paths": "^3.9.0",
    "tsup": "^8.0.2",
    "typescript": "5.1.6",
    "webpack-graphql-loader": "1.0.2",
    "yaml-loader": "0.6.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,}": [
      "eslint --max-warnings=10"
    ],
    "src/**/*.{json,md}": [
      "eslint --max-warnings=10"
    ],
    ".env": [
      "ts-node ./scripts/verifyEnv.ts"
    ]
  }
}