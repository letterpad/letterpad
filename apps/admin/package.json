{
  "name": "admin",
  "version": "4.1.2",
  "author": "Abhishek Saha",
  "license": "MIT",
  "prisma": {
    "seed": "node ./src/graphql/db/dist/index.js"
  },
  "eslint": {
    "dirs": [
      "src"
    ]
  },
  "scripts": {
    "seedIfNotSeeded": "node ./src/graphql/db/dist/seedIfNoSeed.js",
    "preinstall": "yarn config set ignore-engines true",
    "codegen": "graphql-let",
    "prepare": "yarn prisma:generate && yarn prisma:migrate:run && yarn seedIfNotSeeded",
    "client": "concurrently -k 'yarn dev'",
    "dev": "next",
    "postbuild": "tsup ./src/graphql/db/seed/seedIfNoSeed.ts ./src/graphql/db/seed/index.ts --format cjs --outDir ./src/graphql/db/dist",
    "prestart": "yarn prepare",
    "build": "yarn codegen && next build",
    "test": "yarn codegen && jest --runInBand",
    "start": "next start",
    "seed": "yarn prisma:seed",
    "lint": "next lint",
    "lint:fix": "next lint --fix && yarn format",
    "migrate": "prisma migrate dev --name init",
    "typecheck": "yarn tsc",
    "prisma:generate": "dotenv scripts/generatePrisma.sh",
    "prisma:migrate:create": "dotenv scripts/createMigration.sh",
    "prisma:migrate:run": "dotenv scripts/runMigration.sh",
    "prisma:seed": "node ./src/graphql/db/dist/index.js",
    "prisma:studio": "dotenv scripts/studio.sh",
    "cy:open": "cypress open --env SECRET_KEY='some-secret' --browser chrome",
    "cy:run": "cypress run --record --key e2c7bae0-47ed-49e5-8ea8-b0c26c3493f7 --browser chrome",
    "e2e:ci": "yarn prisma:seed && start-server-and-test 'yarn start' http://localhost:3000/admin/login cy:run",
    "e2e": "yarn prisma:seed && start-server-and-test 'yarn dev' http://localhost:3000/admin/login cy:open",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "find-deadcode": "ts-prune | grep -Ev \"\\.d\\.ts|__generated__|seed\" | cat"
  },
  "dependencies": {
    "@apollo/client": "^3.7.1",
    "@dnd-kit/core": "^6.0.5",
    "@dnd-kit/sortable": "^7.0.1",
    "@dnd-kit/utilities": "^3.2.0",
    "@floating-ui/react-dom-interactions": "^0.10.2",
    "@grammarly/editor-sdk-react": "^2.3.2",
    "@graphql-tools/load-files": "6.5.3",
    "@graphql-tools/merge": "8.2.6",
    "@graphql-tools/schema": "8.3.5",
    "@graphql-tools/utils": "^8.10.0",
    "@prisma/client": "^4.11.0",
    "@root/greenlock": "v4",
    "@stripe/stripe-js": "^1.42.1",
    "@tinymce/tinymce-react": "^4.3.0",
    "@types/bcryptjs": "^2.4.2",
    "acme-http-01-webroot": "^3.0.0",
    "ajv": "^8.10.1",
    "ajv-formats": "^2.1.1",
    "apollo-server": "^3.6.3",
    "apollo-server-micro": "3.11.1",
    "axios": "^1.2.0",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0-rc.10",
    "classnames": "^2.3.2",
    "clipboard": "^2.0.11",
    "cloudinary": "^1.28.1",
    "copy-dir": "^1.3.0",
    "cross-fetch": "^3.1.5",
    "cryptr": "^6.0.2",
    "debug": "^4.3.1",
    "dotenv": "^16.0.1",
    "dotenv-cli": "^5.1.0",
    "eslint-config-custom": "*",
    "graphql": "16.6.0",
    "graphql-request": "^3.4.0",
    "image-size": "^0.9.7",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.3.0",
    "micro": "^9.4.1",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
    "next-auth": "^4.19.0",
    "nodemailer": "^6.7.3",
    "nprogress": "^0.2.0",
    "prisma": "4.9.0",
    "raygun": "^0.13.1",
    "react-colorful": "^5.6.1",
    "react-google-recaptcha-v3": "^1.10.0",
    "react-icons": "^4.6.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-sticky-box": "^1.0.2",
    "react-tag-autocomplete": "^6.3.0",
    "reading-time": "^1.3.0",
    "rollbar": "^2.25.1",
    "showdown": "^2.0.0",
    "sqlite3": "^5.0.11",
    "stripe": "^10.15.0",
    "swr": "1.3.0",
    "tinymce": "^6.3.1",
