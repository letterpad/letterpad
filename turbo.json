{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build",
        "graphql-letterpad#codegen"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "dependsOn": [
        "letterpad-sdk#build",
        "graphql-letterpad#codegen"
      ],
      "cache": false
    },
    "prisma:generate": {
      "outputs": [
        "node_modules/.prisma/client"
      ]
    },
    "test": {
      "outputs": [
        "coverage/**"
      ],
      "inputs": [
        "admin/**/*.tsx",
        "admin/**/*.ts",
        "client/**/*.ts",
        "client/**/*.tsx"
      ]
    },
    "e2e:ci": {
      "outputs": [],
      "inputs": [
        "admin/**/*.tsx",
        "admin/**/*.ts",
        "client/**/*.ts",
        "client/**/*.tsx"
      ]
    },
    "e2e": {
      "outputs": []
    },
    "codegen": {
      "outputs": []
    },
    "typecheck": {
      "outputs": []
    },
    "lint:fix": {
      "outputs": []
    },
    "start": {
      "outputs": []
    },
    "find-deadcode": {
      "outputs": []
    },
    "seed": {
      "outputs": []
    }
  }
}