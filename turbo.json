{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "clean": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "prepare": {
      "dependsOn": ["letterpad-graphql#build"],
      "cache": false
    },
    "dev": {
      "dependsOn": ["letterpad-sdk#build", "letterpad-graphql#build"],
      "cache": false
    },
    "prisma:generate": {
      "outputs": ["node_modules/.prisma/client"]
    },
    "test": {
      "outputs": ["coverage/**"],
      "inputs": [
        "admin/**/*.tsx",
        "admin/**/*.ts",
        "client/**/*.ts",
        "client/**/*.tsx"
      ]
    },
    "e2e:ci": {
      "outputs": [],
      "inputs": [
        "admin/**/*.tsx",
        "admin/**/*.ts",
        "client/**/*.ts",
        "client/**/*.tsx"
      ]
    },
    "e2e": {
      "outputs": []
    },
    "codegen": {
      "outputs": []
    },
    "typecheck": {
      "outputs": []
    },
    "lint:fix": {
      "outputs": []
    },
    "start": {
      "outputs": []
    },
    "find-deadcode": {
      "outputs": []
    },
    "seed": {
      "outputs": ["dist/**"]
    }
  }
}
